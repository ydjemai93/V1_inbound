# LiveKit Configuration
LIVEKIT_URL=wss://<project-id>.livekit.cloud
LIVEKIT_API_KEY=<your-api-key>
LIVEKIT_API_SECRET=<your-api-secret>

# LiveKit SIP Host (généralement dérivé de l'URL)
LIVEKIT_SIP_HOST=<project-id>.sip.livekit.cloud

# SIP Configuration
OUTBOUND_TRUNK_ID=<outbound-trunk-id>
INBOUND_TRUNK_ID=<inbound-trunk-id>
DISPATCH_RULE_ID=<dispatch-rule-id>

# Twilio Configuration
TWILIO_ACCOUNT_SID=<twilio-account-sid>
TWILIO_AUTH_TOKEN=<twilio-auth-token>
TWILIO_PHONE_NUMBER=<twilio-phone-number>

# AI Services
OPENAI_API_KEY=<openai-api-key>
DEEPGRAM_API_KEY=<deepgram-api-key>
CARTESIA_API_KEY=<cartesia-api-key>
